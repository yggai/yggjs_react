{
  "name": "yggjs_react",
  "version": "1.0.0",
  "description": "专为React快速开发打造的脚手架工具 - A scaffolding tool for rapid React development",
  "author": "源滚滚",
  "license": "MIT",
  "keywords": [
    "react",
    "scaffold",
    "template",
    "generator",
    "vite",
    "typescript",
    "zustand",
    "react-router",
    "cli-tool"
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "bin": {
    "yggjs_react": "./dist/cli.js",
    "ggr": "./dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./basic": {
      "import": "./dist/basic/index.esm.js",
      "require": "./dist/basic/index.js",
      "types": "./dist/basic/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "build:watch": "vite build --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "example:basic": "cd examples/basic && pnpm dev",
    "example:basic:build": "cd examples/basic && pnpm build",
    "example:basic:install": "cd examples/basic && pnpm install --registry=https://registry.npmmirror.com",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "eslint": "^8.53.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "terser": "^5.43.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.3"
  },
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "fs-extra": "^11.1.1",
    "chalk": "^5.3.0",
    "ora": "^7.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yuangungun/yggjs_react.git"
  },
  "bugs": {
    "url": "https://github.com/yuangungun/yggjs_react/issues"
  },
  "homepage": "https://github.com/yuangungun/yggjs_react#readme"
}
